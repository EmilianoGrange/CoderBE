<div class="container">
    <h1>Products</h1>
    <hr>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th class="h3">Producto</th>
                <th class="h3">Descripcion</th>
                <th class="h3">Precio</th>
                <th class="h3">Codigo</th>
                <th class="h3">Stock</th>
                <th class="h3">Categoria</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>

    <a href="/realTimeProducts" class="btn btn-default">Ir a Real Time Products</a>
</div>